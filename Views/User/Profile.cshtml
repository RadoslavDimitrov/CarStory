@model ProfileUserViewModel;


<table>
    <thead>
        <tr>
            <th>Username:</th>
            <th>Email:</th>
            <th>Role:</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.UserName</td>
            <td>@Model.Email</td>
            <td>@Model.RoleName</td>
